{
  "name": "abideguide",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "mobile": "expo start",
    "ios": "expo start --ios",
    "android": "expo start --android",
    "web": "expo start --web",
    "web:build": "expo export:web",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "postinstall": "prisma generate && patch-package",
    "db:start": "docker-compose up -d",
    "db:stop": "docker-compose down",
    "db:reset": "docker-compose down -v && docker-compose up -d && npm run db:migrate:reset",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:reset": "prisma migrate reset",
    "db:studio": "prisma studio",
    "db:push": "prisma db push",
    "generate:favicon": "tsx scripts/generate-favicon.ts"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/react": "^11.14.0",
    "@mantine/core": "^8.3.6",
    "@mantine/form": "^8.3.6",
    "@mantine/hooks": "^8.3.6",
    "@mantine/notifications": "^8.3.6",
    "@mantine/tiptap": "^8.3.6",
    "@prisma/client": "^6.18.0",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@rneui/base": "^4.0.0-rc.7",
    "@rneui/themed": "^4.0.0-rc.8",
    "@tabler/icons-react": "^3.35.0",
    "@tiptap/extension-task-item": "^3.10.4",
    "@tiptap/extension-task-list": "^3.10.4",
    "@tiptap/react": "^3.10.4",
    "@tiptap/starter-kit": "^3.10.4",
    "babel-preset-expo": "~54.0.9",
    "bcryptjs": "^3.0.3",
    "dotenv": "^17.2.3",
    "expo": "^54.0.30",
    "expo-constants": "~18.0.12",
    "expo-linking": "~8.0.11",
    "expo-router": "~6.0.21",
    "expo-status-bar": "~3.0.9",
    "next": "^16.1.1",
    "next-auth": "^5.0.0-beta.30",
    "pg": "^8.11.3",
    "prisma": "^6.18.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-icons": "^5.5.0",
    "react-native": "0.81.5",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-vector-icons": "^10.3.0",
    "react-native-web": "^0.21.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@expo/webpack-config": "^18.0.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "~18.2.45",
    "@types/react-dom": "~18.0.10",
    "@types/react-native": "^0.73.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-preset-expo": "~10.0.0",
    "baseline-browser-mapping": "^2.8.32",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "eslint-config-prettier": "^10.1.8",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^30.2.0",
    "patch-package": "^8.0.1",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^3.7.3",
    "sharp": "^0.34.5",
    "tailwindcss": "^4",
    "to-ico": "^1.0.1",
    "tsx": "^4.20.6",
    "typescript": "^5",
    "undici": "^7.16.0"
  }
}
